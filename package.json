{
  "name": "cra-boilerplate",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p start-js",
    "start:debug": "npm-run-all -p  start-js -- --inspect-brk",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:fixjs": "./node_modules/.bin/eslint --ext .js --ext .jsx ./src --fix",
    "lint:js": "./node_modules/.bin/eslint --ext .js --ext .jsx ./src"
  },
  "dependencies": {
    "connected-react-router": "^6.9.1",
    "invariant": "^2.2.4",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "rc-dropdown": "^3.2.0",
    "rc-menu": "^9.0.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-rating": "^2.0.5",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0"
  }
}
