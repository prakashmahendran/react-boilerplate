{
  "name": "cra-boilerplate",
  "version": "4.0.5",
  "private": false, 
  "scripts": {
    "start-js": "react-scripts start",
    "start-dummy-server": "node dummy-server.js",
    "start": "npm-run-all -p start-dummy-server start-js",
    "start:debug": "npm-run-all -p  start-js -- --inspect-brk",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:css": "./node_modules/stylelint/bin/stylelint.js './src/**/*.jsx' || exit 0",
    "lint:fixjs": "./node_modules/.bin/eslint --ext .js --ext .jsx ./src --fix",
    "lint:js": "./node_modules/.bin/eslint --ext .js --ext .jsx ./src"
  },
  "devDependencies": {
    "react-scripts": "3.4.3",
     "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
  },
  "dependencies": {
     "connected-react-router": "^6.8.0",
     "lodash": "^4.17.20",
     "node-sass": "^4.14.1",
    "prettier": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.0",
    "react-rating": "^2.0.5",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
